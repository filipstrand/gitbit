{
  "name": "gitbit",
  "displayName": "GitBit",
  "description": "A lightweight and performant Git graph for Cursor and VS Code, inspired by JetBrains IDEs.",
  "publisher": "filipstrand",
  "author": {
    "name": "Filip Strand"
  },
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/filipstrand/gitbit"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "SCM Providers",
    "Visualization"
  ],
  "keywords": [
    "git",
    "graph",
    "history",
    "visualize",
    "scm"
  ],
  "icon": "media/logo.png",
  "activationEvents": [],
  "main": "./dist/extension/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "gitbit-container",
          "title": "GitBit",
          "icon": "$(graph)"
        }
      ]
    },
    "views": {
      "gitbit-container": [
        {
          "type": "webview",
          "id": "gitbit.view",
          "name": "GitBit"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "bundle:webview": "esbuild src/webview/index.tsx --bundle --outfile=dist/webview.js --format=iife --target=es2020 --minify --sourcemap",
    "bundle:webview:watch": "npm run bundle:webview -- --watch",
    "prepare:codicons": "node scripts/copy-codicons.js",
    "package": "npm run compile && npm run bundle:webview && npm run prepare:codicons",
    "dev": "npm run watch & npm run bundle:webview:watch"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/vscode": "^1.75.0",
    "esbuild": "^0.17.11",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.44",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
